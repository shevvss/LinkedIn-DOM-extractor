{
  "manifest_version": 3,
  "name": "LinkedIn DOM Extractor",
  "version": "1.0",
  "description": "Extension for extracting DOM from LinkedIn",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.linkedin.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon32.png",
      "128": "icon128.png"
    }
  }
}
